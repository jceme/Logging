{
  "name": "Logging",
  "version": "2.0.0",
  "description": "Logging facility based on SLF4J in Java",
  "main": "js/Log.js",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "dependencies": {
    "RelaxedJsonParser": "git://github.com/jceme/RelaxedJsonParser.git#1.0.0"
  },
  "devDependencies": {
    "coffee-script": "1.6.x",
    "istanbul": "0.1.x",
    "mocha": "1.14.x",
    "should": "2.1.x"
  },
  "scripts": {
    "pretest": "rm -rf js; ./node_modules/.bin/coffee -c -o js src/ && ./node_modules/.bin/coffee -c -o js/test test/",
    "test": "./node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u tdd -R spec js/test/**/*Test.js",
    "_test": "./node_modules/.bin/mocha -u tdd -R spec debug js/test/**/*Test.js"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/jceme/Logging.git"
  },
  "keywords": [
    "logging"
  ],
  "author": "Daniel",
  "license": "GPL"
}
